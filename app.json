{
  "expo": {
    "name": "first-app",
    "slug": "first-app",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/logo_unnes.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo_unnes.png",
      "backgroundColor": "white",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timotiusm17.firstapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "08a60cca-ef27-419f-b340-e36f58d91817"
      }
    }
  }
}
